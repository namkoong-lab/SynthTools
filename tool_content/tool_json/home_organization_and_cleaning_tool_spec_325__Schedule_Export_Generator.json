{
  "tool_name": "Schedule Export Generator",
  "tool_description": "Exports finalized cleaning schedules in various formats suitable for calendars, apps, or printed checklists.",
  "parameters": {
    "schedule_id": {
      "type": "string",
      "required": true,
      "description": "ID of schedule to export"
    },
    "export_format": {
      "type": "string",
      "required": true,
      "description": "Output format (calendar, checklist, summary, detailed)"
    },
    "include_supplies": {
      "type": "boolean",
      "required": false,
      "description": "Whether to include supply lists",
      "default": true
    },
    "include_time_estimates": {
      "type": "boolean",
      "required": false,
      "description": "Whether to include duration estimates",
      "default": true
    }
  },
  "error_messages": [
    "Invalid schedule_id: Schedule does not exist",
    "Invalid export_format: Must be one of [calendar, checklist, summary, detailed]"
  ],
  "usage": "Export your finalized schedule in the format that works best for your planning system.",
  "output_details": {
    "export_content": {
      "type": "string",
      "description": "Formatted schedule content"
    },
    "export_summary": {
      "type": "string",
      "description": "Overview of exported schedule"
    }
  }
}